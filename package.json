
  "name": "wms-consultant-agent",
  "version": "1.0.0",
  "description": "AI-Powered WMS Implementation Consultant Agent",
  "main": "server.js",
  "homepage": "https://vickyvigu17.github.io/WMS-Agent",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js || node server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build && cp -r build/* ../public/",
    "install-all": "npm install && cd client && npm install",
    "deploy": "npm run build && npm start",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "test": "cd client && npm test",
    "clean": "rm -rf public/* && rm -rf client/build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "wms",
    "ai",
    "consultant",
    "questions",
    "research",
    "warehouse",
    "management",
    "react",
    "nodejs"
  ],
  "author": "WMS AI Team",
